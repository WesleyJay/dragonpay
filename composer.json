{
    "name": "crazymeeks/dragonpay",
    "description": "A PHP library for Dragonpay payment gateway",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jclaud",
            "email": "jeffclaud17@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "mockery/mockery": "^1.2",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.6"
    },
    "autoload": {
        "psr-4": {
            "Crazymeeks\\": "src",
            "Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "crazymeeks/dragonpay",
                "version": "v1.0.0",
                "source": {
                    "url": "git://github.com/crazymeeks/dragonpay",
                    "type": "git",
                    "reference": "v1.1.4"
                },
                "autoload": {
                    "psr-4": {
                        "Crazymeeks\\": "src"
                    }
                }
            }
            
        }
    ]
}
